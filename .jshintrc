{
  "-W041": false,  /* Disable "Use '===' to compare with 0". */
  "-W058": true,  /* Disable 'Missing () invoking a constructor' */
  "-W126": false,  /* Disable 'Unnecessary grouping operator.' */
  "newcap": false,
  "curly": true,
  "eqeqeq": false,
  "funcscope": true,
  "nocomma": true,
  "noarg": true,
  "nonbsp": true,
  "nonew": true,
  "shadow": false,
  "singleGroups": true,
  "undef": true,
  "unused": "vars",
  "esnext": true,  
  "globals": {
    "require": true,
    "console": true
  }
}
